{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataFactoryMW1"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"OurWorldInData_Corona_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/owid/covid-19-data/blob/master/public/data/owid-covid-data.csv?raw=true"
		},
		"Worldometers_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.worldometers.info/coronavirus/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
